{
  "name": "cc-vf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile alias && cp -r server/templates dist-server/ && cp -r server/p_templates dist-server/",
    "server": "node --max-http-header-size=80000 ./dist-server/bin/boot",
    "server-debug": "node --inspect=9229 --max-http-header-size=80000 --inspect ./dist-server/bin/www",
    "server-debug-worker": "node --inspect=9230 --max-http-header-size=80000 --inspect ./dist-server/bin/worker",
    "server-profile": "node --prof ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server-debug ",
    "dev-worker": "NODE_ENV=development npm-run-all build server-debug-worker ",
    "profile": "NODE_ENV=development npm-run-all build server-profile ",
    "prod_old": "NODE_ENV=production npm-run-all build server",
    "prod": "NODE_ENV=production npm run server",
    "worker": "NODE_ENV=production node dist-server/bin/worker.js",
    "transpile": "babel ./server --out-dir dist-server",
    "heroku-postbuild": "cd ui && npm install && npm run build",
    "alias": "mkdir dist-server/ui && cd dist-server/ui &&  ln -s ../../ui/build .",
    "clean": "rimraf dist-server",
    "test": "jest",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@akryum/flexsearch-es": "^0.7.32",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@cdxoo/dbscan": "^1.1.1",
    "@elastic/elasticsearch": "^8.10.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^6.12.0",
    "@mongodb-js/zstd": "^1.2.2",
    "@postlight/parser": "^2.2.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "agglo": "^0.0.1",
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "bullmq": "^4.2.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.0",
    "compromise": "^14.13.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.7",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "~4.16.1",
    "express-validator": "^7.2.1",
    "fuse.js": "^7.0.0",
    "googleapis": "^112.0.0",
    "gpt-3-encoder": "^1.1.4",
    "html-to-text": "^9.0.5",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^3.0.5",
    "https-proxy-agent": "^7.0.5",
    "install": "^0.13.0",
    "jade": "~1.11.0",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "memory-streams": "^0.1.3",
    "konva": "^9.3.3",
    "ml-hclust": "^3.1.0",
    "ml-kmeans": "^6.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.10.0",
    "morgan": "~1.9.1",
    "msgpackr": "^1.10.1",
    "natural": "^7.0.5",
    "node-cache": "^5.1.2",
    "node-html-parser": "^7.0.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^2.0.20",
    "npm": "^9.7.2",
    "npm-run-all": "^4.1.5",
    "openai": "^4.0.0",
    "parse-full-name": "^1.2.6",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0",
    "passport-oauth2-refresh": "^2.1.0",
    "pdf.js-extract": "^0.2.1",
    "pdfkit": "^0.13.0",
    "puppeteer": "^22.10.0",
    "puppeteer-core": "^21.1.1",
    "redis": "^4.6.8",
    "rimraf": "^3.0.2",
    "skmeans": "^0.11.3",
    "socket.io": "^4.7.0",
    "stream-json": "^1.9.1",
    "stripe": "^18.3.0",
    "turndown": "^7.2.0",
    "usehooks-ts": "^3.1.1",
    "winston": "^3.17.0",
    "yahoo-finance2": "^2.9.0"
  },
  "optionalDependencies": {
    "skia-canvas": "^1.0.0"
  },
  "overrides": {
    "stream-shift": "1.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "sourceMaps": "inline",
    "retainLines": true
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/dist-server/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2"
  }
}
